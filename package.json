{
    "name": "git-int-commit-plugin",
    "displayName": "git-int-commit-plugin",
    "description": "Automatically generate git commit",
    "version": "0.0.2",
    "engines": {
        "vscode": "^1.42.0"
    },
    "publisher": "redjue",
    "license": "MIT",
    "homepage": "https://github.com/RedJue/git-int-commit-plugin/blob/master/README.md",
    "bugs": {
        "url": "https://github.com/RedJue/git-int-commit-plugin/issues"
    },
    "icon": "assets/logo.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/RedJue/git-int-commit-plugin"
    },
    "keywords": [
        "git",
        "commit",
        "vscode"
    ],
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.showGitCommit"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "extension.showGitCommit",
                "title": "show git commit template",
                "icon": {
                    "dark": "assets/icon.svg",
                    "light": "assets/icon_light.svg"
                }
            }
        ],
        "menus": {
            "scm/title": [
                {
                    "when": "scmProvider == git",
                    "command": "extension.showGitCommit",
                    "group": "navigation"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "eslint src --ext ts",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^7.0.1",
        "@types/node": "^12.11.7",
        "@types/vscode": "^1.42.0",
        "@typescript-eslint/eslint-plugin": "^2.18.0",
        "@typescript-eslint/parser": "^2.18.0",
        "eslint": "^6.8.0",
        "glob": "^7.1.6",
        "mocha": "^7.0.1",
        "typescript": "^3.7.5",
        "vscode-test": "^1.3.0"
    },
    "dependencies": {}
}
